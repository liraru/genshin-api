{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#008065",
    "activityBar.background": "#008065",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#e480ff",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#008065",
    "statusBar.background": "#004d3d",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#008065",
    "statusBarItem.remoteBackground": "#004d3d",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#004d3d",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#004d3d99",
    "titleBar.inactiveForeground": "#e7e7e799",
    "editorGroup.border": "#008065",
    "panel.border": "#008065",
    "statusBar.debuggingBackground": "#4d0010",
    "statusBar.debuggingForeground": "#e7e7e7"
  },
  "peacock.color": "#004D3D",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.matchBrackets": "always",
  "editor.guides.bracketPairs": "active",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "prettier.configPath": ".prettierrc",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "files.exclude": {
    "dist": true,
    "**/*.spec.ts": true,
    "e2e": true,
    ".idea": true,
    "lib": true,
    "node_modules": true
  }
}